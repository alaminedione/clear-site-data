{
  "manifest_version": 3,
  "name": "Supprimer Données Site",
  "description": "Effacer les données de navigation d'un site web spécifique.",
  "version": "1.0",
  "permissions": [
    "cookies",
    "browsingData",
    "storage",
    "tabs",
    "webNavigation"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "options_page": "options.html",
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ]
}
